{
  "name": "antd-starter",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "max dev",
    "postinstall": "max setup",
    "analyze": "cross-env ANALYZE=1 umi dev",
    "start:nomock": "cross-env MOCK=none umi dev",
    "start:prod.addone": "cross-env UMI_ENV=prod.addone umi dev",
    "build": "cross-env UMI_ENV=prod max build",
    "build:local": "cross-env UMI_ENV=local max build",
    "build:dev": "cross-env UMI_ENV=dev max build",
    "build:test": "cross-env UMI_ENV=test max build",
    "build:local.ssr": "cross-env UMI_ENV=local.ssr max build",
    "build:prod.ssr": "cross-env UMI_ENV=prod.ssr max build",
    "build:prod.cdn": "cross-env UMI_ENV=prod.cdn max build",
    "build:prod.addone": "cross-env UMI_ENV=prod.addone max build",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "dependencies": {
    "@hocgin/browser-addone-kit": "^2.0.12",
    "@hocgin/hkit": "^2.0.12",
    "@hocgin/mock-kit": "^2.0.12",
    "@hocgin/ui": "^5.0.3",
    "antd": "5.0.3",
    "classnames": "^2.3.1",
    "query-string": "^7.0.1"
  },
  "resolutions": {
    "@types/react": "17.0.44"
  },
  "devDependencies": {
    "@hocgin/umijs-plugin-browser-addone": "^2.0.12",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "@types/mockjs": "^1.0.6",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@umijs/max": "^4.0.0",
    "cross-env": "^7.0.3",
    "lint-staged": "^10.0.7",
    "mockjs": "^1.1.0",
    "prettier": "^2.2.0",
    "react": "17.x",
    "react-dom": "17.x",
    "typescript": "^4.1.2",
    "yorkie": "^2.0.0"
  }
}
