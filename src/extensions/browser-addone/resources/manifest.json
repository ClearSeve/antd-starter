{
  "manifest_version": 3,
  "name": "插件名称",
  "description": "插件用途简述",
  "version": "3.1.9",
  "author": "hocgin",
  "homepage_url": "https://www.hocgin.top",
  "omnibox": {
    "keyword": "hocgin"
  },
  "icons": {
    "16": "static/logo.jpg",
    "32": "static/logo.jpg",
    "48": "static/logo.jpg",
    "128": "static/logo.jpg"
  },
  "action": {
    "default_icon": {
      "19": "static/logo.jpg"
    },
    "default_title": "天气",
    "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "options_ui": {
    "page": "options.html"
  },
  "devtools_page": "devtools.html",
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content-script.js"
      ],
      "css": [
        "content-script.css"
      ],
      "run_at": "document_start"
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "jquery-2.1.1.min.js"
      ],
      "matches": [
        "https://*/*"
      ]
    }
  ],
  "permissions": [
    "tabs",
    "activeTab",
    "scripting",
    "contextMenus",
    "webRequest",
    "webRequestBlocking",
    "storage",
    "http://*/*",
    "https://*/*",
    "notifications"
  ],
  "host_permissions": [
    "<all_urls>"
  ]
}
